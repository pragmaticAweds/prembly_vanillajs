{
  "name": "prembly-identitypass-js-sdk",
  "version": "0.0.0",
  "description": "Prembly identity javascript sdk",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "tsc",
    "es-lint": "eslint . --ext .ts",
    "rebuild": "rm -rf dist && yarn build",
    "test": "jest",
    "ci": "yarn run lint && yarn run es-lint && yarn run test && yarn run build",
    "release": "yarn run lint && yarn run es-lint && yarn run test && yarn run build && changeset publish"
  },
  "keywords": [
    "javascript",
    "verification",
    "biometric",
    "radar",
    "global"
  ],
  "author": "Pragmatic Aweds",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "tsup": "^6.6.3",
    "typescript": "*"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "axios": "^1.3.4",
    "eslint": "^8.36.0"
  }
}
